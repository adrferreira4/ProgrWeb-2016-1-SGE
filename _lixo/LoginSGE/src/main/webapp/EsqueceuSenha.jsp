<%@ page language="java" contentType="text/html; charset=ISO-8859-1" pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<head>
<meta charset="UTF-8" />
<title>SGE - Esqueci a Senha</title>
</head>
<body>
	<div class="container">
		<div class="codrops-top">
			<strong>SGE - Sistema de Gestao Escolar </strong>
		</div>
		<section>
		<div id="container_demo">
			<div id="wrapper">
				<div id="register" class="animate form">
					<form>
						<h1>Recuperar Senha</h1>
						<p>
							<label for="username" class="uname" data-icon="u"> Usuário </label>
							<input id="username" name="username" required="required" type="text" placeholder="Insira seu usuário aqui..." />
						</p>
						<p>
							<label for="username" class="uname" data-icon="u"> E-mail </label>
							<input id="username" name="username" required="required" type="email" placeholder="Insira seu e-mail institucional aqui..." />
						</p>
						<p class="signin button">
							<input type="submit" value="Nova Senha" />
						</p>
						<p class="change_link">
							<a href="Login.jsp" class="to_login">Fazer Login</a>
						</p>
					</form>
				</div>
			</div>
		</div>
		</section>
	</div>
</body>
</html>